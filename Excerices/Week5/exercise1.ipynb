   "source": [
    "from itertools import product\n",
    "(K, M) = map(int, input().strip().split())\n",
    "N = (list(map(int,input().strip().split()))[1:] for _ in range(K))\n",
    "results =list(map(lambda x: sum(i**2 for i in x)%M, product(*N)))\n",
    "print(max(results))"
   ]
